{
  "restApiRoot": "/api",
  "host": "tinhha-blog.herokuapp.com",
  "port": 8080,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": true,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false
}
